    "react": "^18.3.1",
    "react-bootstrap": "^2.10.5",
    "react-dom": "^18.3.1",
    "recharts": "^2.13.3"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
